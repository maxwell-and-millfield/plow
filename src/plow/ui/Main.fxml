<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane dividerPositions="0.3" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="plow.controllers.MainController">
	<items>
		<AnchorPane SplitPane.resizableWithParent="false">
			<children>
				<ListView fx:id="playlistsView" AnchorPane.bottomAnchor="0.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0" />
			</children>
		</AnchorPane>
		<AnchorPane>
			<children>
				<TableView fx:id="tracksTable" AnchorPane.bottomAnchor="0.0"
					AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
					AnchorPane.topAnchor="0.0">
					<columns>
						<TableColumn fx:id="titleColumn" minWidth="20.0"
							prefWidth="200.0" text="Title" />
						<TableColumn fx:id="artistColumn" minWidth="20.0"
							prefWidth="100.0" text="Artist" />
						<TableColumn fx:id="filenameColumn" minWidth="20"
							prefWidth="300.0" text="Filename" />
					</columns>
				</TableView>
			</children>
		</AnchorPane>
	</items>
</SplitPane>
